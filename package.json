{
  "name": "studyusa-portal",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "babel-transpile": "babel --ignore .c9,.git,docs,heroku,node_modules,images,notifJson,old,styles,views,.babelrc,.gitignore,package.json,.env -d ./build . -s",
    "copy-untranspiled": "cp -R ./views ./images ./notifJson ./styles ./package.json ./package-lock.json ./.env ./build/",
    "build": "npm run clean && npm run babel-transpile && npm run copy-untranspiled",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node app.js",
    "build-run-local": "npm run build && cp -R ./node_modules ./build/ && cd ./build && heroku local",
    "start-local": "heroku local"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaks19/studyusa-portal.git"
  },
  "author": "Adarsh K S Jeewajee",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jaks19/studyusa-portal/issues"
  },
  "homepage": "https://github.com/jaks19/studyusa-portal#readme",
  "dependencies": {
    "aws-sdk": "^2.85.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "body-parser": "^1.15.2",
    "busboy": "^0.2.13",
    "connect-flash": "^0.1.1",
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "fs": "0.0.1-security",
    "lodash": "^4.17.10",
    "method-override": "^2.3.7",
    "mongodb": "^2.2.12",
    "mongoose": "^4.6.8",
    "mongoose-deep-populate": "^3.1.1",
    "multiparty": "^4.1.2",
    "nodemailer": "^4.6.8",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.0.0",
    "request": "^2.79.0",
    "stripe": "^4.13.0",
    "tingle.js": "^0.13.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current",
            "browsers": "> 0.25%, not dead"
          }
        }
      ]
    ]
  }
}
