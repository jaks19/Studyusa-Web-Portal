{
  "name": "studyusa-portal",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel --ignore .c9,git,data,docs,heroku,node_modules,uploads,views,.babelrc,.gitignore,mongod,package.json,.env -d ./build . -s",
    "copy-unbuilt": "cp -R ./views ./images ./notifJson ./package.json ./.env ./build/",
    "build": "npm run clean && npm run build-server && npm run copy-unbuilt",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./build/app.js",
    "build-and-start": "npm run build && heroku local",
    "refresh-front-and-start": "rm -rf ./build/views ./build/frontend && cp -R ./views ./front-end ./build/ && heroku local",
    "refresh-front": "rm -rf ./build/views ./build/frontend && cp -R ./views ./front-end ./build/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaks19/studyusa-portal.git"
  },
  "author": "Adarsh K S Jeewajee",
  "license": "ISC",
  "bugs": {
