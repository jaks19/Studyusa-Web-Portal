<div class='col-lg-11 square' align='center' style='border: 0.5px #d5d5d5 solid; margin: 10%; margin-top: 0%; border-radius:2%;'>
    <% if (!user.admin){ %>
        <h5>Welcome</h5><h4><%=user.name %></h4>
    <% } else { %>
        <h5> Admin View of </h5><h4><%=client.name %></h4>
    <% } %>
    
    <div class="card" style='width: 60%;'>
        <img class='card-img-top' src="/images/funny-profile-pictures.jpg" style='max-width: 100%; max-height: 100%; border-radius:1%;' />
    </div>

    <% if (!user.admin){ %>
        <a href='#' class='btn btn-xs' style='width:100%;'>Edit Profile</a>
        <a href='#' class='btn btn-xs' style='width:100%;'>Edit Intake Information</a>
    <% } %>
</div>